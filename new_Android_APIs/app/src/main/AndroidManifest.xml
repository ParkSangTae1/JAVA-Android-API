<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="me.blog.eyeballss.android_api">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activities.MyFragmentManagerSampleActivity" />
        <activity android:name=".Activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activities.MyLoggerSampleActivity" />
        <activity android:name=".Activities.MyCalendarSampleActivity" />
        <activity android:name=".Activities.MyRecyclerViewSampleActivity" />
        <activity android:name=".Activities.MyCustomViewSampleActivity" />
        <activity android:name=".Activities.MyDataPasserSenderSampleActivity" />
        <activity android:name=".Activities.MyDataPasserReceiverSampleActivity" />
        <activity android:name=".Activities.MyFileManagerSampleActivity" />
        <activity android:name=".Activities.MyLoaderSampleActivity"></activity>
    </application>

</manifest>